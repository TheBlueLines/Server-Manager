<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <RootNamespace>Developer_Tools</RootNamespace>
    <Nullable>enable</Nullable>
    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
    <ApplicationIcon>data-server.ico</ApplicationIcon>
    <NoWin32Manifest>true</NoWin32Manifest>
    <StartupObject>Developer_Tools.Program</StartupObject>
    <Version>0.1</Version>
    <Title>Server Manager</Title>
    <Authors>TheBlueLines</Authors>
    <Company>TTMC Corporation</Company>
    <Product>Server Manager</Product>
    <Description>Application for server management</Description>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="data-server.ico" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="Auram">
      <HintPath>..\Auram\API\bin\Debug\net8.0\Auram.dll</HintPath>
    </Reference>
    <Reference Include="Tools">
      <HintPath>..\Tools\bin\Debug\net8.0\Tools.dll</HintPath>
    </Reference>
  </ItemGroup>

</Project>